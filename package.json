{
	"name": "vutable",
	"author": "shawroger",
	"description": "A easy and cool tool to transform a csv file into webpage",
	"version": "1.0.1",
	"main": "src/main.ts",
	"unpkg": "dist/vutable.js",
	"files": [
		"dist",
		"docs",
		"src",
		"public",
		".gitignore",
		"postcss.config.js",
		"tsconfig.json",
		"package-lock.json",
		"README.md"
	],
	"homepage": "https://github.com/shawroger/vutable",
	"scripts": {
		"dev": "parcel public/index.html",
		"build": "parcel build public/index.html --no-content-hash --no-source-maps"
	},
	"dependencies": {
		"@vue/composition-api": "^0.3.4",
		"axios": "^0.19.2",
		"element-ui": "^2.13.0",
		"parcel-plugin-pre-clean": "^1.0.1",
		"rexine": "^2.3.0",
		"vue": "^2.6.10",
		"vue-axios": "^2.1.5",
		"vue-hot-reload-api": "^2.3.4"
	},
	"devDependencies": {
		"@vue/component-compiler-utils": "^3.1.1",
		"autoprefixer": "^9.7.4",
		"cssnano": "^4.1.10",
		"less": "^3.0.4",
		"parcel-plugin-static-files-copy": "^2.3.1",
		"postcss": "^7.0.27",
		"typescript": "~3.5.3",
		"parcel-bundler": "^1.12.4",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"rules": {},
		"parserOptions": {
			"parser": "@typescript-eslint/parser"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"parcelClean": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "public",
		"watcherGlob": "**",
		"excludeGlob": "**/*.html"
	}
}
